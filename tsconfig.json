{
    "compilerOptions": {
        "target": "ES6",
        "module": "CommonJS",
        "lib": ["es6", "dom"],
        "noImplicitThis": true,
        "strictNullChecks": true,
        "jsx": "react",
        "esModuleInterop": true,
        "noImplicitAny": true,
        "allowSyntheticDefaultImports": true,    
    },
    "include": ["src/**/*"]
